{
  "name": "LATUCT",
  "version": "1.0",
  "description": "Launch A Tab Using Current Token",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["contentscript.js"]
    }
  ],
  "manifest_version": 2
}
